# Gemini AI API Key
GEMINI_API_KEY=your_gemini_api_key_here

# Google Sheets Credentials (path to credentials.json file)
GOOGLE_SHEETS_CREDENTIALS=./credentials.json

# Google Spreadsheet ID
SPREADSHEET_ID=your_spreadsheet_id_here

# User ID for the bot
USER_ID=your_user_id_here

# Authorized phone numbers (comma-separated, with country code)
# Example: 6281234567890,6289876543210
AUTHORIZED_NUMBERS=your_phone_numbers_here

# Dashboard API Integration (set to true to enable)
ENABLE_DASHBOARD_API=true 
